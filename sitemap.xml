---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url }}</loc>
    </url>
    <url>
        <loc>{{ site.url }}/about</loc>
    </url>
    <url>
        <loc>{{ site.url }}/music</loc>
    </url>
    <url>
        <loc>{{ site.url }}/books</loc>
    </url>

    {% for post in site.posts %}
    <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
    </url>
    {% endfor %}
</urlset>
